[
  {
    "$group": {
      "_id": {
        "__alias_0": "$PRODUCT_ID"
      },
      "__alias_1": {
        "$addToSet": "$CALL_ID"
      }
    }
  },
  {
    "$project": {
      "_id": 0,
      "__alias_0": "$_id.__alias_0",
      "__alias_1": {
        "$size": "$__alias_1"
      }
    }
  },
  {
    "$project": {
      "x": "$__alias_1",
      "y": "$__alias_0",
      "_id": 0
    }
  },
  {
    "$addFields": {
      "__agg_sum": {
        "$sum": [
          "$x"
        ]
      }
    }
  },
  {
    "$sort": {
      "__agg_sum": 1
    }
  },
  {
    "$project": {
      "__agg_sum": 0
    }
  },
  {
    "$limit": 5000
  }
]
 
